{
  "name": "vue-ts-quasar",
  "version": "0.0.1",
  "description": "a production-ready front-end solution based on Vue 3 with TS",
  "productName": "Quasar TS Admin Template",
  "author": "psi <cyril@wendyourway.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron -- --no-sandbox --disable-setuid-sandbox",
    "build": "quasar clean && quasar build",
    "deploy": "push-dir --dir=dist/spa --remote=origin --branch=deploy",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "vitest --config './vitest/vitest.config.ts'",
    "coverage": "vitest run --coverage --config './vitest/vitest.config.ts'"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.3",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.10",
    "@quasar/app-vite": "^1.0.0-beta.8",
    "@quasar/vite-plugin": "^1.0.7",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.22",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vue/devtools": "^6.1.3",
    "@vue/test-utils": "^2.0.0-rc.17",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "electron": "^17.2.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "push-dir": "^0.4.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.4",
    "vite-plugin-inspect": "^0.4.3",
    "vitest": "^0.7.11"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
